function cwiczenie2(zestaw)
    for p = 1:10
        fprintf("p = %d\t", p);
        [ARr,Vr]=licz_wsp_AR(zestaw ,p);
        dc_12 = DC(ARr, 1, 2);
        fprintf("DC^2_12 = %d\t", dc_12)
        dc_21 = DC(ARr, 2, 1);
        fprintf("DC^2_21 = %d\n", dc_21)
    end
end

function dc = DC(ARr, i, j)
    dc = sum(ARr(i,j, :).^2,3);
end

function a = aij(ARr, i, j)
    a = ARr(i, j, :);
end
